			label="COM_ETATSFIN_ENTREPRISE_FIELD_ADRESSE_LABEL"
			description="COM_ETATSFIN_ENTREPRISE_FIELD_ADRESSE_DESC"
			size="40"
			class="inputbox"
			required="false"
			default=""
